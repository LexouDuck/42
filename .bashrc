alias dir="ls --color=auto --format=long"
# Long list
alias ll="ls -l"
# List all except '.' and '..'
alias la="ls -al"

# Runs the 42FileChecker
alias 42FileChecker="sh ~/42FileChecker/42FileChecker.sh"

# Checks the current state of the git repository for the current directory
alias git_check="git ls-tree --full-tree -r HEAD"

# Compiling with flags in a fast and easy way
alias compile="gcc -o main -Wall -Wextra -Werror"

# 42 program aliases for windows
alias fdf="startxwin ./fdf"
alias fractol="startxwin ./fractol"
